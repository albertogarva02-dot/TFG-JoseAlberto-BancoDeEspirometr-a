    background-color: #4A4A4A; /* Gris oscuro para el botón */
    border: 1px solid #5A5A5A;
}
QPushButton#Breiniciar:hover, QPushButton#Bseleccionararchivo:hover, QPushButton#Bposicioninicial:hover {
    background-color: #666666; /* Gris más claro */
}

/* =================================================================== */
/* ==                  ESTILO DE LA TABLA (QTableWidget)            == */
/* =================================================================== */

QTableWidget {
    background-color: #252526; /* Fondo de la tabla ligeramente distinto al de la app */
    border: 1px solid #404040;
    border-radius: 5px;
    gridline-color: #383838; /* Líneas de la rejilla sutiles */
    font-size: 13px;
    color: #E0E0E0;
}

/* --- Estilo de la Cabecera (Títulos de Columna) --- */
QHeaderView::section {
    background-color: #2D2D2D; /* Cabecera oscura */
    color: #61AFEF; /* Texto azul claro */
    padding: 8px;
    border: none;
    border-bottom: 2px solid #0078D7;
    font-weight: bold;
}

/* --- Estilo de las Celdas y Filas --- */
QTableWidget::item {
    padding: 8px 10px;
    border: none;
    border-bottom: 1px solid #303030; /* Separador de filas muy sutil */
}

/* --- Zebra-Striping --- */
QTableWidget::item:alternate {
    background-color: #2D2D2D; /* Filas alternas un poco más claras */
}

/* --- Estilo de la Fila/Celda Seleccionada --- */
QTableWidget::item:selected {
    background-color: #0078D7; /* Azul corporativo */
    color: white;
}

/* --- Corner Button (La esquina superior izquierda de la tabla) --- */
QTableCornerButton::section {
    background-color: #2D2D2D;
    border: none;
}

/* =================================================================== */
/* ==                  ESTILO DE LAS BARRAS DE SCROLL               == */
/* =================================================================== */

QScrollBar:vertical, QScrollBar:horizontal {
    border: none;
    background-color: #1E1E1E; /* Fondo del carril */
    width: 12px;
    height: 12px;
    margin: 0px;
}

/* --- Estilo del manejador --- */
QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background-color: #4A4A4A; /* Gris medio */
    min-height: 20px;
    min-width: 20px;
    border-radius: 6px;
    margin: 2px; /* Margen para que parezca flotante */
}
QScrollBar::handle:vertical:hover, QScrollBar::handle:horizontal:hover {
    background-color: #666666; /* Gris más claro al interactuar */
}

/* --- Ocultar botones --- */
QScrollBar::add-line, QScrollBar::sub-line {
    height: 0px;
    width: 0px;
    background: none;
}
QScrollBar::add-page, QScrollBar::sub-page {
    background: none;
}</string>
