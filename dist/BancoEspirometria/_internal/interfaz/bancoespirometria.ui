/* =================================================================== */

QWidget {
    /* Fuente preferida */
    font-family: &quot;Segoe UI&quot;, Arial, &quot;Helvetica Neue&quot;, sans-serif;
    
    /* FONDO OSCURO PRINCIPAL: Un gris muy oscuro, estilo VS Code o JetBrains */
    background-color: #1E1E1E; 
    
    /* TEXTO PRINCIPAL: Blanco suave (no blanco puro para no dañar la vista) */
    color: #E0E0E0; 
}

/* =================================================================== */
/* ==                     ESTILO DE WIDGETS                         == */
/* =================================================================== */

/* --- Contenedores y Grupos --- */
QGroupBox {
    font-size: 14px;
    font-weight: bold;
    /* Azul más claro (Cyan/Azul Cielo) para que resalte sobre el negro */
    color: #61AFEF; 
    border: 1px solid #404040; /* Borde gris oscuro sutil */
    border-radius: 8px;
    margin-top: 12px;
}
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 0 10px;
    background-color: #1E1E1E; /* Debe coincidir con el fondo de la ventana */
}

/* --- Etiquetas de texto --- */
QLabel {
    background-color: transparent;
    font-size: 13px;
    color: #E0E0E0;
}

/* Títulos específicos que tenías definidos */
QLabel#Ltitulo {
    font-size: 24px;
    font-weight: bold;
    color: #61AFEF; /* Azul claro brillante */
}
QLabel#Lnombre {
    font-size: 15px;
    font-weight: bold;
    color: #61AFEF;
}

/* --- Campos de Entrada y Desplegables --- */
QLineEdit, QComboBox, QLineEdit:disabled {
    font-size: 13px;
    padding: 6px 10px;
    border: 1px solid #555555; /* Borde gris medio */
    border-radius: 5px;
    
    /* Fondo de los inputs ligeramente más claro que el fondo de la app */
    background-color: #2D2D2D; 
    color: #FFFFFF; /* Texto blanco puro al escribir */
}

QLineEdit:disabled {
    background-color: #333333;
    color: #888888;
}

QLineEdit:focus, QComboBox:focus {
    /* Mantenemos el borde azul corporativo al seleccionar */
    border: 2px solid #0078D7; 
}

QComboBox::drop-down {
    border-left: 1px solid #555555;
    width: 20px;
}
/* Flecha del combo box (opcional, para forzar que sea visible en oscuro) */
QComboBox::down-arrow {
    image: none;
    border-left: 5px solid transparent;
    border-right: 5px solid transparent;
    border-top: 5px solid #E0E0E0;
    margin: 5px;
}


/* --- Campos de Texto Multilínea (Logs, etc.) --- */
QTextEdit, QPlainTextEdit {
    font-size: 13px;
    padding: 6px 10px;
    border: 1px solid #555555;
    border-radius: 5px;
    background-color: #2D2D2D;
    color: #E0E0E0;
}
QTextEdit:focus, QPlainTextEdit:focus {
    border: 2px solid #0078D7;
}

/* --- Pestañas (Tabs) --- */
QTabWidget::pane {
    border-top: 2px solid #0078D7;
}
QTabBar::tab {
    background-color: #2D2D2D; /* Pestaña inactiva oscura */
    color: #AAAAAA; /* Texto grisáceo */
    padding: 10px;
    min-width: 100px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    font-weight: bold;
    border: 1px solid #404040;
    border-bottom: none;
}
QTabBar::tab:selected {
    background-color: #0078D7; /* Pestaña activa en azul vibrante */
    color: white;
    border: 1px solid #0078D7;
}

/* =================================================================== */
/* ==                      ESTILO DE BOTONES                        == */
/* =================================================================== */

/* --- Estilo Base para TODOS los Botones --- */
QPushButton {
    font-size: 13px;
    font-weight: bold;
    border-radius: 5px;
    padding: 7px 12px;
    color: white; /* Siempre blanco en botones sólidos */
    border: 1px solid transparent;
}

/* --- Botón por Defecto (Azul) --- */
QPushButton {
    background-color: #0078D7;
}
QPushButton:hover {
    background-color: #198CE6; /* En modo oscuro, el hover suele ser MÁS CLARO para brillar */
    border: 1px solid #61AFEF;
}
QPushButton:pressed {
    background-color: #005A9C;
}

/* --- Botón de Acción Positiva (Verde) --- */
QPushButton#Bcomenzarmedida, QPushButton#Bejecutarsimulacion, QPushButton#Brealizarmovimiento {
    background-color: #28A745;
}
QPushButton#Bcomenzarmedida:hover, QPushButton#Bejecutarsimulacion:hover, QPushButton#Brealizarmovimiento:hover {
    background-color: #34C759; /* Verde más brillante al pasar el ratón */
}

/* --- Botón de Acción Negativa (Rojo) --- */
QPushButton#Bdetenermovimiento, QPushButton#Beliminardatos, QPushButton#Bdetenersimulacion, QPushButton#Bfinalizarmedida {
    background-color: #DC3545;
}
QPushButton#Bdetenermovimiento:hover, QPushButton#Beliminardatos:hover, QPushButton#Bdetenersimulacion:hover, QPushButton#Bfinalizarmedida:hover {
    background-color: #FF4554; /* Rojo más brillante al pasar el ratón */
}

/* --- Botón Secundario (Gris) --- */
QPushButton#Breiniciar, QPushButton#Bseleccionararchivo, QPushButton#Bposicioninicial {
    background-color: #4A4A4A; /* Gris oscuro para el botón */
    border: 1px solid #5A5A5A;
}
QPushButton#Breiniciar:hover, QPushButton#Bseleccionararchivo:hover, QPushButton#Bposicioninicial:hover {
    background-color: #666666; /* Gris más claro */
}

/* =================================================================== */
/* ==                  ESTILO DE LA TABLA (QTableWidget)            == */
/* =================================================================== */

QTableWidget {
    background-color: #252526; /* Fondo de la tabla ligeramente distinto al de la app */
    border: 1px solid #404040;
    border-radius: 5px;
    gridline-color: #383838; /* Líneas de la rejilla sutiles */
    font-size: 13px;
    color: #E0E0E0;
}

/* --- Estilo de la Cabecera (Títulos de Columna) --- */
QHeaderView::section {
    background-color: #2D2D2D; /* Cabecera oscura */
    color: #61AFEF; /* Texto azul claro */
    padding: 8px;
    border: none;
    border-bottom: 2px solid #0078D7;
    font-weight: bold;
}

/* --- Estilo de las Celdas y Filas --- */
QTableWidget::item {
    padding: 8px 10px;
    border: none;
    border-bottom: 1px solid #303030; /* Separador de filas muy sutil */
}

/* --- Zebra-Striping --- */
QTableWidget::item:alternate {
    background-color: #2D2D2D; /* Filas alternas un poco más claras */
}

/* --- Estilo de la Fila/Celda Seleccionada --- */
QTableWidget::item:selected {
    background-color: #0078D7; /* Azul corporativo */
    color: white;
}

/* --- Corner Button (La esquina superior izquierda de la tabla) --- */
QTableCornerButton::section {
    background-color: #2D2D2D;
    border: none;
}

/* =================================================================== */
/* ==                  ESTILO DE LAS BARRAS DE SCROLL               == */
/* =================================================================== */

QScrollBar:vertical, QScrollBar:horizontal {
    border: none;
    background-color: #1E1E1E; /* Fondo del carril */
    width: 12px;
    height: 12px;
    margin: 0px;
}

/* --- Estilo del manejador --- */
QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background-color: #4A4A4A; /* Gris medio */
    min-height: 20px;
    min-width: 20px;
    border-radius: 6px;
    margin: 2px; /* Margen para que parezca flotante */
}
QScrollBar::handle:vertical:hover, QScrollBar::handle:horizontal:hover {
    background-color: #666666; /* Gris más claro al interactuar */
}

/* --- Ocultar botones --- */
QScrollBar::add-line, QScrollBar::sub-line {
    height: 0px;
    width: 0px;
    background: none;
}
QScrollBar::add-page, QScrollBar::sub-page {
    background: none;
}</string>
