    font-size: 13px;
    padding: 6px 10px;
    border: 1px solid #555555; /* Borde gris medio */
    border-radius: 5px;
    
    /* Fondo de los inputs ligeramente más claro que el fondo de la app */
    background-color: #2D2D2D; 
    color: #FFFFFF; /* Texto blanco puro al escribir */
}

QLineEdit:disabled {
    background-color: #333333;
    color: #888888;
}

QLineEdit:focus, QComboBox:focus {
    /* Mantenemos el borde azul corporativo al seleccionar */
    border: 2px solid #0078D7; 
}

QComboBox::drop-down {
    border-left: 1px solid #555555;
    width: 20px;
}
